{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": [
      "es2017",
      "esnext.asynciterable",
      "dom"
    ],
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "pretty": false,
    "removeComments": true,
    "outDir": "js"
  },
  "include": [
    "src/*.ts"
  ],
  "exclude": []
}
